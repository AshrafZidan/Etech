<ion-header>

    <ion-navbar>
        <ion-icon class="back-icon" (click)="back()"  name="ios-arrow-round-back-outline"></ion-icon>

        <ion-title class="page-title"> Contact Support </ion-title>  </ion-navbar>

</ion-header>


<ion-content padding>

    <form>

        <ion-list>

            <!-- fullName field -->
            <ion-item class="item1 animated fadeInDown">
              <ion-label stacked  >
                Email Support
              </ion-label>
                <ion-input  class="inpt" type="email" disabled value="Support@domin.com"></ion-input>
            </ion-item>


            <ion-item class="item2 animated fadeInDown">
              <ion-label stacked  >
               Phone Support
              </ion-label>
              <ion-input  type="tel" class="inpt" disabled value="123456789"></ion-input>
          </ion-item>

</ion-list>
</form>


</ion-content>
