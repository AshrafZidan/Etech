page-payment {

  .card-width{
    border: 0;
    width: 40px;
    margin-top: 13px;
  }
  .datetime-md , .datetime-ios{
    padding: 0 !important;
     padding-right: 6px !important;
     padding-top: 8px !important;
     padding-left: 8px !important;

  }
  .card{

    margin-top: 13px;
  }
  .card-content{
    display: inline-flex;


    // border-radius: 10px;

  }
  .card-radio{
    margin-top:4px !important;
  }
  .card-logo{
  display: inline-block;
    margin-left: 7px;
    // padding-top: 1px;
    margin-right: 10px;
    ion-icon{
      font-size: .999em;
    }
  }


  .card-name{
    font-size: 13px;
    font-weight: 600;
    margin: 0;
    margin-top: 15px;

    p{
      margin: 0;
    }


  }
  .che{
    display: inline-block;
    float: right;
    margin: 10px;
  }

  .checkbox-ios .checkbox-checked {
    border-color: #32db64;
    background-color: #32db64;
  }

  .card-number{
    display: none;
    margin-top: 20px;
    // padding-left: 0 !important;
    // padding: 8px;
    padding-bottom: 0px;
    padding-top: 2px;
    background: #f8f8f8;
    .card-number-inpt{
      background: none;
    padding-left: 15px;
    padding-right: 6px;
      .labl{

    padding-left: 0;
    margin-left: 5px;
    margin-bottom: 13px;
    font-size: 16px;
    color: #777;
      }
      .card-inpt{
        border: 1px solid #ccc;
        color: #000 !important;
        background: #fff;
        border-radius: 7px;
        border-bottom: none !important;
        // padding: 8px;
        // padding-right: 6px;
        margin-bottom:0 !important;
        min-height: 42px;

      }
      .text-input , .text-input:placeholder{
        color:#000 !important;
      }
    }
  }


}


.check-out{
  background:#fff;
  -webkit-box-shadow: -1px -5px 5px 0px rgba(235,228,235,1);
-moz-box-shadow: -1px -5px 5px 0px rgba(235,228,235,1);
box-shadow: -1px -5px 5px 0px rgba(235,228,235,1);
width: 100%;
bottom: 0;
position: fixed !important;
left: 0;
padding: 13px 7px;
  .total{
    // display: inline-flex;
    margin-bottom: 20px;
    .subtotal{
      display: inline;
      margin-top: 0;
      margin-bottom: 0;
      margin-right: 15px;
    font-size: 15px;
    span{
      font-size: 14px;
      color: #969696;
    }

    }
    .pricer{
      float: right;
      font-weight: bold;
      letter-spacing: .9px;
      display: inline;
      margin-top: 0;
      margin-bottom: 0;
      margin-left: 15px;
      font-size: 20px;
      padding-right: 10px;


    }
  }
}

ion-app.ios [padding], ion-app.ios [padding] .scroll-content {
  padding: 16px;
  // padding-bottom: 100px !important;
}




.text-input::-moz-placeholder {
  color: #999;
}

.text-input:-ms-input-placeholder {
  color: #999;
}

.text-input::-webkit-input-placeholder {
  text-indent: 0;
  color: #999;
}


.radio-md .radio-checked {
  border-color: #32db64;
}
.radio-md .radio-icon  {

 width: 20px;
 height: 20px;
}
.radio-md .radio-inner  {
left: 3px;
top: 3px;
border-radius: 50%;
position: absolute;
width: 10px;
height: 10px;
// background-color: #3982b2;
background-color: #32db64;

}

.txt{
  font-size: 17px;
  font-weight: 500;
  margin-top: 0px;
}
page-payment .text-input {
  /* color: #fff !important; */

  color: #666 !important;
  font-size: 16px;
  padding-left: 5px !important;
}

page-payment .text-input::-moz-placeholder {
  color: #666 !important;
  font-size: 16px;
}

page-payment .text-input:-ms-input-placeholder {
  color: #666 !important;
  font-size: 16px;
}

page-payment  .text-input::-webkit-input-placeholder {
  text-indent: 0;
  font-size: 16px;
  color: #666 !important;
}
.show{
  display: block !important;
}
.c-card{
  margin-top: 11px !important;

}
.card-option{
  margin-top: 2px;
  display: inline;
}

.txt-small{
  font-size: 11px !important;
}
.text-card{
      padding-left: 20px;
    font-size: 13px;
    letter-spacing: .5px;
    margin-bottom:0;

}
.text-card::after{
  content: '';
    height: 1px;
    background: #dedede;
    display: block;
    padding-right: 10px;
    margin-top: 12px;
    width: 90%;

    }

.list-unstyledcs{
  list-style: none;
      margin-top: 10px;
    padding-left: 20px;
}
.list-unstyledcs li{
    margin-bottom: 8px;
    font-weight: 500;
    letter-spacing: .6px;
    font-size: 13px;
    }

 .list-unstyledcs   .bold{
      font-weight: 600;
    }